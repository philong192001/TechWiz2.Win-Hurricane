    <section class="page-section">
        <div class="container">

            <div class="col-md-8 text-left">

                <h2 class="block-title"><span>FeedBack Form</span></h2>

                <!-- Contact form -->
                <form name="contact-form" method="post" asp-action="Index" class="contact-form" id="contact-form">
                    <div class="outer required">
                        <div class="form-group af-inner">
                            <label class="sr-only" for="subject">Subject</label>
                            <input  type="text" name="Title" id="subject" placeholder="Subject" value="" size="30"
                                   data-toggle="tooltip" title="Subject is required"
                                   class="form-control placeholder" />
                        </div>
                    </div>

                    <div class="form-group af-inner">
                        <label class="sr-only" for="input-message">Message</label>
                        <textarea name="Description" id="input-message" placeholder="Description" rows="4" cols="50"
                                  data-toggle="tooltip" title="Message is required"
                                  class="form-control placeholder"></textarea>
                    </div>

                    <div class="outer required">
                        <div class="form-group af-inner">
                            <input type="submit" name="submit" class="form-button form-button-submit btn btn-theme btn-theme-dark" id="submit_btn" value="Send feedback" />
                        </div>
                    </div>

                </form>
                <!-- /Contact form -->

